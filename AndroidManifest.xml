        </activity>
        <activity android:name="com.george.mylifeassistant.weather.search.SearchActivity" >
        </activity>
        <activity android:name="com.george.mylifeassistant.weather.contents.ContentsActivity" >
        </activity>
        <activity android:name="com.george.mylifeassistant.weather.contents.AddContentsActivity" >
        </activity>
        <activity android:name="com.george.mylifeassistant.weather.caculator.CaCulatorAvtivity" >
