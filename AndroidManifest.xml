                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.george.mylifeassistant.weather.WeatherMainActivity" >
        </activity>
